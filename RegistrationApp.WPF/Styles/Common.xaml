<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Colors-->
    <Color x:Key="ColorPrimary1">#353c4a</Color>
    <Color x:Key="ColorPrimary2">#1d232e</Color>
    
    <!--Brushes-->
    <SolidColorBrush x:Key="BrushePrimary1" Color="{StaticResource ColorPrimary1}"/>
    <SolidColorBrush x:Key="BrushePrimary2" Color="{StaticResource ColorPrimary2}"/>
</ResourceDictionary>